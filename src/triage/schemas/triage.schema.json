{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schemas/triage.schema.json",
  "title": "bioslogtriage CLI output",
  "type": "object",
  "required": [
    "schema_version",
    "normalization",
    "events",
    "llm_enabled"
  ],
  "properties": {
    "schema_version": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+\\.[0-9]+$",
      "examples": [
        "0.1.0"
      ]
    },
    "normalization": {
      "type": "object",
      "required": [
        "line_count"
      ],
      "properties": {
        "line_count": {
          "type": "integer",
          "minimum": 0
        },
        "empty_line_count": {
          "type": "integer",
          "minimum": 0
        }
      },
      "additionalProperties": true
    },
    "events": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "event_id",
          "category",
          "severity",
          "confidence",
          "boot_blocking",
          "where",
          "rule_hits",
          "fingerprint"
        ],
        "properties": {
          "event_id": {
            "type": "string"
          },
          "category": {
            "type": "string"
          },
          "subcategory": {
            "type": [
              "string",
              "null"
            ]
          },
          "severity": {
            "type": "string",
            "enum": [
              "fatal",
              "high",
              "medium",
              "low",
              "info"
            ]
          },
          "confidence": {
            "type": "number",
            "minimum": 0.0,
            "maximum": 1.0
          },
          "boot_blocking": {
            "type": "boolean"
          },
          "where": {
            "type": "object",
            "required": [
              "segment_id",
              "line_range"
            ],
            "properties": {
              "segment_id": {
                "type": "string"
              },
              "phase": {
                "type": "string",
                "enum": [
                  "SEC",
                  "PEI",
                  "DXE",
                  "BDS"
                ]
              },
              "line_range": {
                "type": "object",
                "required": [
                  "start",
                  "end"
                ],
                "properties": {
                  "start": {
                    "type": "integer",
                    "minimum": 1
                  },
                  "end": {
                    "type": "integer",
                    "minimum": 1
                  }
                },
                "additionalProperties": false
              },
              "other_lines": {
                "type": "array",
                "items": {
                  "type": "integer",
                  "minimum": 1
                }
              }
            },
            "additionalProperties": false
          },
          "extracted": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            }
          },
          "rule_hits": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "rule_id",
                "weight",
                "match_confidence"
              ],
              "properties": {
                "rule_id": {
                  "type": "string"
                },
                "weight": {
                  "type": "number"
                },
                "match_confidence": {
                  "type": "number",
                  "minimum": 0.0,
                  "maximum": 1.0
                }
              },
              "additionalProperties": false
            }
          },
          "fingerprint": {
            "type": "object",
            "required": [
              "stable_key",
              "dedupe_group"
            ],
            "properties": {
              "stable_key": {
                "type": "string"
              },
              "dedupe_group": {
                "type": "string"
              }
            },
            "additionalProperties": false
          },
          "occurrences": {
            "type": "integer",
            "minimum": 1
          },
          "evidence": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "ref",
                "kind",
                "start_line",
                "end_line"
              ],
              "properties": {
                "ref": {
                  "type": "string"
                },
                "kind": {
                  "type": "string"
                },
                "start_line": {
                  "type": "integer",
                  "minimum": 1
                },
                "end_line": {
                  "type": "integer",
                  "minimum": 1
                },
                "lines": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "required": [
                      "idx",
                      "text"
                    ],
                    "properties": {
                      "idx": {
                        "type": "integer",
                        "minimum": 1
                      },
                      "text": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  }
                }
              },
              "additionalProperties": false
            }
          }
        },
        "additionalProperties": false
      }
    },
    "llm_enabled": {
      "type": "boolean"
    },
    "run_metadata": {
      "type": "object",
      "additionalProperties": true
    },
    "boot_timeline": {
      "type": "object",
      "required": [
        "segments",
        "boot_outcome"
      ],
      "properties": {
        "segments": {
          "type": "array",
          "items": {
            "type": "object",
            "required": [
              "segment_id",
              "start_line",
              "end_line",
              "phases"
            ],
            "properties": {
              "segment_id": {
                "type": "string"
              },
              "start_line": {
                "type": "integer",
                "minimum": 1
              },
              "end_line": {
                "type": "integer",
                "minimum": 1
              },
              "phases": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": [
                    "phase",
                    "start_line",
                    "end_line",
                    "confidence"
                  ],
                  "properties": {
                    "phase": {
                      "type": "string",
                      "enum": [
                        "SEC",
                        "PEI",
                        "DXE",
                        "BDS"
                      ]
                    },
                    "start_line": {
                      "type": "integer",
                      "minimum": 1
                    },
                    "end_line": {
                      "type": "integer",
                      "minimum": 1
                    },
                    "confidence": {
                      "type": "number",
                      "minimum": 0.0,
                      "maximum": 1.0
                    }
                  },
                  "additionalProperties": false
                }
              }
            },
            "additionalProperties": false
          }
        },
        "boot_outcome": {
          "type": "string"
        },
        "boot_blocking_event_id": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "additionalProperties": false
    },
    "signals": {
      "type": "object",
      "additionalProperties": true
    },
    "deterministic_summary": {
      "type": "object",
      "additionalProperties": true
    },
    "llm_synthesis": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
